               # SQL JOIN Queries
  1. INNER JOIN: rentals with valid customers and films
SELECT r.RentalID, c.CustomerName, f.FilmTitle, r.RentalDate, r.ReturnDate
FROM Rentals r
INNER JOIN Customers c ON r.CustomerID = c.CustomerID
INNER JOIN Films f ON r.FilmID = f.FilmID;

   2. LEFT JOIN: customers who never rented a film
SELECT c.CustomerName, r.RentalID
FROM Customers c
LEFT JOIN Rentals r ON c.CustomerID = r.CustomerID
WHERE r.RentalID IS NULL;

   3. LEFT JOIN: films never rented
SELECT f.FilmTitle, r.RentalID
FROM Films f
LEFT JOIN Rentals r ON f.FilmID = r.FilmID
WHERE r.RentalID IS NULL;

   4. FULL OUTER JOIN: compare all customers and films
SELECT c.CustomerName, f.FilmTitle, r.RentalID
FROM Customers c
LEFT JOIN Rentals r ON c.CustomerID = r.CustomerID
LEFT JOIN Films f ON r.FilmID = f.FilmID;

   5. SELF JOIN: customers in the same region
SELECT c1.CustomerName AS CustomerA, c2.CustomerName AS CustomerB, c1.Region
FROM Customers c1
INNER JOIN Customers c2 ON c1.Region = c2.Region AND c1.CustomerID < c2.CustomerID;
  
           # Window Functions

1. Ranking Functions:
--ROW_NUMBER()
SELECT 
    FilmID,
    COUNT(*) AS TotalRentals,
    ROW_NUMBER() OVER (ORDER BY COUNT(*) DESC) AS RowNum
FROM Rentals
GROUP BY FilmID;

--RANK()
SELECT 
    FilmID,
    COUNT(*) AS TotalRentals,
    RANK() OVER (ORDER BY COUNT(*) DESC) AS RentalRank
FROM Rentals
GROUP BY FilmID;

--DENSE_RANK()
SELECT 
    FilmID,
    COUNT(*) AS TotalRentals,
    DENSE_RANK() OVER (ORDER BY COUNT(*) DESC) AS DenseRentalRank
FROM Rentals
GROUP BY FilmID;

-- PERCENT_RANK()
SELECT 
    FilmID,
    COUNT(*) AS TotalRentals,
    PERCENT_RANK() OVER (ORDER BY COUNT(*) DESC) AS PercentRank
FROM Rentals
GROUP BY FilmID;

2. Aggregate Window Functions

-- SUM()
SELECT 
    RentalDate,
    COUNT(*) OVER (
        ORDER BY RentalDate
        ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW
    ) AS RunningTotal
FROM Rentals;

-- AVG()  running average rentals per day
SELECT 
    RentalDate,
    AVG(DailyCount) OVER (
        ORDER BY RentalDate
        ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW
    ) AS RunningAverage
FROM (
    SELECT RentalDate, COUNT(*) AS DailyCount
    FROM Rentals
    GROUP BY RentalDate
) AS DailyTotals;

-- MIN() – earliest rental date so far (RANGE)
SELECT 
    RentalDate,
    MIN(RentalDate) OVER (
        ORDER BY RentalDate
        RANGE BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW
    ) AS FirstRentalSoFar
FROM Rentals;

-- MAX() – latest rental date so far (RANGE)
SELECT 
    RentalDate,
    MAX(RentalDate) OVER (
        ORDER BY RentalDate
        RANGE BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW
    ) AS LastRentalSoFar
FROM Rentals;

3. Navigation Functions

-- LAG() – compare rentals with previous month
SELECT 
    Year,
    Month,
    TotalRentals,
    LAG(TotalRentals) OVER (ORDER BY Year, Month) AS PreviousMonthRentals
FROM (
    SELECT 
        YEAR(RentalDate) AS Year,
        MONTH(RentalDate) AS Month,
        COUNT(*) AS TotalRentals
    FROM Rentals
    GROUP BY YEAR(RentalDate), MONTH(RentalDate)
) AS MonthlyRentals;

-- LEAD() – compare rentals with next month
SELECT 
    Year,
    Month,
    TotalRentals,
    LEAD(TotalRentals) OVER (ORDER BY Year, Month) AS NextMonthRentals
FROM (
    SELECT 
        YEAR(RentalDate) AS Year,
        MONTH(RentalDate) AS Month,
        COUNT(*) AS TotalRentals
    FROM Rentals
    GROUP BY YEAR(RentalDate), MONTH(RentalDate)
) AS MonthlyRentals;

4. Distribution Functions

-- NTILE(4) – customer rental quartiles
SELECT 
    CustomerID,
    COUNT(*) AS TotalRentals,
    NTILE(4) OVER (ORDER BY COUNT(*) DESC) AS RentalQuartile
FROM Rentals
GROUP BY CustomerID;

-- CUME_DIST() – cumulative distribution of customers
SELECT 
    CustomerID,
    COUNT(*) AS TotalRentals,
    CUME_DIST() OVER (ORDER BY COUNT(*) DESC) AS CumulativeDistribution
FROM Rentals
GROUP BY CustomerID;


